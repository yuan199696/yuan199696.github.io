                &#125;],
                selectSrcValue: '',
                selectDistValue: '',
                input_appKey: '23059209eaca30b4',
                input_key: 'S3eW0xliXFpnviLkhhnBym9TFYVDNzVz',
                trans_source: '',
                translation: [],
                translation_show: false,
                basic: [],
                basic_show: false,
                web: [],
                web_show: false
